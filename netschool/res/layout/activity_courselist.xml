    <LinearLayout android:orientation="vertical" android:id="@+id/TopTitleLayout3" android:background="#ff48a75b" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_alignParentTop="true">
        <TextView android:textSize="25.0sp" android:gravity="center" android:id="@+id/TopTitle3" android:background="#00000000" android:fadingEdge="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content" android:singleLine="true" />
    </LinearLayout>
